{
  "version": 2,
  "builds": [
    {
      "src": "app/__init__.py",
      "use": "@now/python"
    },
    {
      "src": "react-app/src/index.js",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/__init__.py"
    },
    {
      "src": "/(.*)",
      "dest": "react-app/src/index.js"
    }
  ]
}
